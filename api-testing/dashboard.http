@baseUrl = http://localhost:3000
@categoryId = 00000000-0000-0000-0000-000000000000

### Get dashboard - default (current month)
GET {{baseUrl}}/api/dashboard
Accept: application/json

### Get dashboard - specific month
GET {{baseUrl}}/api/dashboard?month=2025-10
Accept: application/json

### Get dashboard - specific month (February)
GET {{baseUrl}}/api/dashboard?month=2025-02
Accept: application/json

### Get dashboard - filter by cash account
GET {{baseUrl}}/api/dashboard?account=cash
Accept: application/json

### Get dashboard - filter by card account
GET {{baseUrl}}/api/dashboard?account=card
Accept: application/json

### Get dashboard - filter by all accounts (explicit)
GET {{baseUrl}}/api/dashboard?account=all
Accept: application/json

### Get dashboard - filter by single category
GET {{baseUrl}}/api/dashboard?categoryIds={{categoryId}}
Accept: application/json

### Get dashboard - filter by multiple categories
GET {{baseUrl}}/api/dashboard?categoryIds={{categoryId}},00000000-0000-0000-0000-000000000001
Accept: application/json

### Get dashboard - combined filters (month + account)
GET {{baseUrl}}/api/dashboard?month=2025-10&account=card
Accept: application/json

### Get dashboard - combined filters (month + account + categories)
GET {{baseUrl}}/api/dashboard?month=2025-10&account=card&categoryIds={{categoryId}}
Accept: application/json

### Get dashboard - all filters combined
GET {{baseUrl}}/api/dashboard?month=2025-02&account=cash&categoryIds={{categoryId}}
Accept: application/json

### Get dashboard - invalid month format (missing leading zero)
GET {{baseUrl}}/api/dashboard?month=2025-2
Accept: application/json

### Get dashboard - invalid month format (not YYYY-MM)
GET {{baseUrl}}/api/dashboard?month=2025/10/01
Accept: application/json

### Get dashboard - invalid month (month > 12)
GET {{baseUrl}}/api/dashboard?month=2025-13
Accept: application/json

### Get dashboard - invalid month (month = 0)
GET {{baseUrl}}/api/dashboard?month=2025-00
Accept: application/json

### Get dashboard - invalid account type
GET {{baseUrl}}/api/dashboard?account=invalid
Accept: application/json

### Get dashboard - invalid category UUID
GET {{baseUrl}}/api/dashboard?categoryIds=not-a-uuid
Accept: application/json

